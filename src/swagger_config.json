{
  "template": {
    "swagger": "2.0",
    "info": {
      "title": "Todo API",
      "description": "API for managing todos",
      "version": "1.0.0"
    },
    "basePath": "/",
    "schemes": [
      "http",
      "https"
    ],
    "consumes": [
      "application/json"
    ],
    "produces": [
      "application/json"
    ]
  },
  "swagger_config": {
    "headers": [],
    "specs": [
      {
        "endpoint": "apispec",
        "route": "/apispec.json"
      }
    ],
    "static_url_path": "/flasgger_static",
    "swagger_ui": true,
    "specs_route": "/docs"
  },
  "info": {
    "title": "POD Booking API",
    "description": "Swagger documentation for the POD Booking System backend (Flask + SQLAlchemy on SQL Server)."
  }
}